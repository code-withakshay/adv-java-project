package com.codeWithAkshay.www;



public class CollectionApp 
{
	public static void main(String[] args)
	{
		int n=2;
		for(int i=1;i<=4;i++)
		{
			boolean flag=true;
			for(int j=2;j<=n/2;j++)
			{	
				if(n%j==0)
				{
					flag=false;
					break;
				}
			}
			if(flag)
			{
				System.out.print(n+" ");
				n++;
				break;
			}
			else
			{
			 n++;
			}
			System.out.println();
		}
	}
}
