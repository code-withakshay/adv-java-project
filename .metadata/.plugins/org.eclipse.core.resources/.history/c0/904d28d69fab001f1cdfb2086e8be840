package com.codeWithAkshay.www;

public class Student 
{
	private int id;
    private String name;
    private String add;

    public Student(int id, String name, String add) {
        this.id = id;
        this.name = name;
        this.add = add;
    }

	public int getId() {
		return id;
	}

	public String getName() {
		return name;
	}

	public String getAdd() {
		return add;
	}

   
}

